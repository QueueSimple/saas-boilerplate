    "better-sqlite3": "^9.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "openai": "^4.28.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "stripe": "^14.14.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
